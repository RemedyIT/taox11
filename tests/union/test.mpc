// -*- MPC -*-

project(*union_gen_Idl): ridl_ostream_defaults, taox11_anytypecode {
  IDL_Files {
    test.idl
  }
  custom_only = 1
}

project(*union_gen_Server): taox11_server, taox11_anytypecode {
  after += *union_gen_Idl
  Source_Files {
    foo.cpp
    server.cpp
  }
  Source_Files {
    testC.cpp
    testS.cpp
  }
  specific(vs2022) {
    LanguageStandard = stdcpp20
  }
}

project(*union_gen_Client): taox11_client, taox11_anytypecode {
  after += *union_gen_Idl
  Source_Files {
    client.cpp
  }
  Source_Files {
    testC.cpp
  }
  specific(vs2022) {
    LanguageStandard = stdcpp20
  }
}

